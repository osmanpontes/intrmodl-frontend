{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "rules": {
    "semi": ["error", "never"],

    "import/prefer-default-export": "warn",

    "react/prefer-stateless-function": "warn",
    "react/prop-types": "warn",
    "react/jsx-filename-extension": "off"
  }
}
